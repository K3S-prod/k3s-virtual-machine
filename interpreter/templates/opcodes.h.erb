#ifndef OPCODES_H
#define OPCODES_H

#include <cstdint>

namespace k3s {

enum class Opcode : uint8_t {
<%- ISA.opcode_groups.each do |group_name, group| -%>
    // <%= group_name %>
    <%- group.each do |signature| -%>
        // <%= signature["signature"] %>
        <%- signature["opc"].each do |opcode| -%>
        <%= opcode.upcase %>,
        <%- end -%>
    <%- end -%>
<% end %>
};

}  // namespace k3s

#endif  // OPCODES_H
